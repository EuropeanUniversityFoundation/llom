{
    "name": "euf/llom",
    "description": "LLOM - Language Learning App",
    "version": "1.0.0",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Jeroen van Lent",
            "email": "jeroen.vanlent@uni-foundation.eu"
        }
    ],
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "drupal/h5p",
                "type": "drupal-module",
                "version": "dev-8.x-1.x",
                "source": {
                    "type": "git",
                    "url": "https://git.drupalcode.org/project/h5p.git",
                    "reference": "471d2b95f73f93e24866892d31a6ea1e30afd3f3"
                }
            }
        },
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8",
            "exclude": ["drupal/h5p"]
        }
    ],
    "require": {
        "composer/installers": "^1.9",
        "cweagans/composer-patches": "^1.6.5",
        "drupal/admin_toolbar": "^3.0",
        "drupal/adminimal_admin_toolbar": "^1.10",
        "drupal/adminimal_theme": "^1.6",
        "drupal/allow_iframed_site": "^3.0",
        "drupal/bootstrap": "^3.21",
        "drupal/core-composer-scaffold": "^9.2",
        "drupal/core-recommended": "^9.2",
        "drupal/h5p": "dev-8.x-1.x",
        "drupal/languagefield": "^1.5",
        "drupal/material_icons": "^1.0",
        "drupal/module_filter": "^3.1",
        "drupal/recaptcha": "^3.0",
        "drupal/redirect_after_login": "^2.6",
        "drupal/registration_role": "^1.1",
        "drupal/restui": "^1.19",
        "drupal/smtp": "^1.0@beta",
        "drupal/upgrade_status": "^3.11",
        "drush/drush": "^10.0.0",
        "oomphinc/composer-installers-extender": "^2.0",
        "vlucas/phpdotenv": "^2.4",
        "webflo/drupal-finder": "^1.0.0",
        "webmozart/path-util": "^2.3",
        "zaporylie/composer-drupal-optimizations": "^1.0"
    },
    "require-dev": {
        "drupal/core-dev": "^9.2",
        "drupal/devel": "^4.1"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ],
        "files": ["load.environment.php"]
    },
    "scripts": {
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    },
    "extra": {
        "composer-exit-on-patch-failure": true,
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": ["type:drupal-library"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/Commands/{$name}": ["type:drupal-drush"]
        },
        "patches": {
            "drupal/h5p": {
                "Code assumes score is int, not stored in case of double": "https://www.drupal.org/files/issues/2019-10-09/3086807_1.patch",
                "Drupal 9 compatibility": "https://www.drupal.org/files/issues/2020-04-19/d9-compatibility-3129142-3.patch"
            }
        },
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            }
        }
    }
}
